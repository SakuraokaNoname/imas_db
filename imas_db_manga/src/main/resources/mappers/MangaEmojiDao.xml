<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.db.imas.dao.MangaEmojiDao">

    <select id="getMangaEmojiList" resultType="com.db.imas.model.entity.MangaEmoji">
        SELECT *
        FROM manga_emoji
        WHERE `type` = #{type}
    </select>

</mapper>